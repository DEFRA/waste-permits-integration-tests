{
  "name": "waste-permits-integration-tests",
  "version": "1.19.0",
  "engines": {
    "node": "14.7.0"
  },
  "description": "This package runs integrations tests for the DEFRA Waste Permits Service https://github.com/DEFRA/waste-permits",
  "author": "Department for Environment, Food & Rural Affairs",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "test:firefox-desktop-test-smoke-dev": "node ./node_modules/.bin/cucumber-js ./features -p firefox-desktop-test-smoke-dev -f @cucumber/pretty-formatter -f json:features/reports/cucumber_report_desktop.json",
    "test:firefox-desktop-test-smoke-test": "node ./node_modules/.bin/cucumber-js ./features -p firefox-desktop-test-smoke-test -f @cucumber/pretty-formatter -f json:features/reports/cucumber_report_desktop.json",
    "test:chrome-desktop-test-smoke-dev": "node ./node_modules/.bin/cucumber-js --parallel 2 ./features -p chrome-desktop-test-smoke-dev -f @cucumber/pretty-formatter -f json:features/reports/cucumber_report_desktop.json",
    "test:chrome-desktop-test-smoke-test": "node ./node_modules/.bin/cucumber-js ./features -p chrome-desktop-test-smoke-test -f @cucumber/pretty-formatter -f json:features/reports/cucumber_report_desktop.json",
    "test:chrome-desktop-test-smoke-preprod": "node ./node_modules/.bin/cucumber-js ./features -p chrome-desktop-test-smoke-preprod -f @cucumber/pretty-formatter -f json:features/reports/cucumber_report_desktop.json",
    "test:chrome-desktop-test-smoke-prod": "node ./node_modules/.bin/cucumber-js --parallel 2 ./features -p chrome-desktop-test-smoke-prod -f @cucumber/pretty-formatter -f json:features/reports/cucumber_report_desktop.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEFRA/waste-permits-integration-tests.git"
  },
  "bugs": {
    "url": "https://github.com/ajaxscape/waste-permits-integration/issues"
  },
  "homepage": "https://github.com/ajaxscape/waste-permits-integration#readme",
  "dependencies": {
    "@cucumber/cucumber": "^7.3.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "ajv": "^8.6.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "colors": "^1.4.0",
    "command-line-args": "^5.1.1",
    "cucumber-html-reporter": "^5.4.0",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "geckodriver": "^2.0.1",
    "iedriver": "^3.150.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "require-all": "^3.0.0",
    "selenium-webdriver": "^4.0.0-beta.4 "
  },
  "devDependencies": {
    "adal-node": "^0.2.2",
    "chromedriver": "^91.0.1",
    "standard": "^16.0.3"
  },
  "standard": {
    "ignore": [
      "node_modules/"
    ]
  }
}
