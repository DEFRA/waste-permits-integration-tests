  "scripts": {
    "test:firefox-desktop-test-smoke-dev": "node node_modules/cucumber/bin/cucumber.js ./features -p firefox-desktop-test-smoke-dev -f json:features/reports/cucumber_report_desktop.json",
    "test:firefox-desktop-test-smoke-test": "node node_modules/cucumber/bin/cucumber.js ./features -p firefox-desktop-test-smoke-test -f json:features/reports/cucumber_report_desktop.json",
